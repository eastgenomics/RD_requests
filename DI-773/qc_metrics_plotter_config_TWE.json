{
    "project_search": {
        "assay": "TWE",
        "pattern": "^038.*TWE38$",
        "after_date": null,
        "before_date": null,
        "mode": "regexp",
        "number_of_projects": 20
    },
    "file": {
        "flagstat": {
            "pattern": "multiqc_samtools_flagstat.txt",
            "file_sep": "\t",
            "plots": [
                {
                    "col_name": "mapped_passed",
                    "y_range_low": null,
                    "y_range_high": null,
                    "plot_failed": true,
                    "warning_line": null,
                    "fail_line": null,
                    "plot_std": true
                },
                {
                    "col_name": "with mate mapped to a different chr_passed",
                    "y_range_low": null,
                    "y_range_high": null,
                    "plot_failed": true,
                    "warning_line": null,
                    "fail_line": null,
                    "plot_std": true
                },
                {
                    "col_name": "supplementary_passed",
                    "y_range_low": null,
                    "y_range_high": null,
                    "plot_failed": true,
                    "warning_line": null,
                    "fail_line": null,
                    "plot_std": true
                },
                {
                    "col_name": "properly paired_passed_pct",
                    "y_range_low": null,
                    "y_range_high": null,
                    "plot_failed": true,
                    "warning_line": null,
                    "fail_line": null,
                    "plot_std": true
                }
            ]
        },
        "verifybamid": {
            "pattern": "multiqc_verifybamid.txt",
            "file_sep": "\t",
            "plots": [
                {
                    "col_name": "FREEMIX",
                    "y_range_low": null,
                    "y_range_high": null,
                    "plot_failed": true,
                    "warning_line": [0.02],
                    "fail_line": [0.05],
                    "plot_std": true
                }
            ]
        },
        "picard": {
            "pattern": "multiqc_picard_HsMetrics.txt",
            "file_sep": "\t",
            "plots": [
                {
                    "col_name": "PCT_TARGET_BASES_20X",
                    "y_range_low": null,
                    "y_range_high": null,
                    "plot_failed": true,
                    "warning_line": [0.95],
                    "fail_line": [0.90],
                    "plot_std": true
                },
                {
                    "col_name": "FOLD_80_BASE_PENALTY",
                    "y_range_low": null,
                    "y_range_high": null,
                    "plot_failed": true,
                    "warning_line": [1.6],
                    "fail_line": null,
                    "plot_std": true
                },
                {
                    "col_name": "FOLD_ENRICHMENT",
                    "y_range_low": null,
                    "y_range_high": null,
                    "plot_failed": true,
                    "warning_line": null,
                    "fail_line": [33, 44],
                    "plot_std": true
                }
            ]
        },
        "het_hom": {
            "pattern": ".*.vcf.QC",
            "file_sep": "\t",
            "plots":[
                {
                    "col_name": "mean_het",
                    "y_range_low": null,
                    "y_range_high": null,
                    "plot_failed": true,
                    "warning_line": null,
                    "fail_line": [0.45, 0.50],
                    "plot_std": true
                }
            ]
        },
        "sentieon": {
            "pattern": "multiqc_sentieon_insertSize.txt",
            "file_sep": "\t",
            "plots": [
                {
                    "col_name": "MEDIAN_INSERT_SIZE",
                    "y_range_low": null,
                    "y_range_high": null,
                    "plot_failed": true,
                    "warning_line": null,
                    "fail_line": null,
                    "plot_std": true
                }
            ]
        },
        "qc_status": {
            "pattern": "^(?i).*_QC_Status\\.xlsx$",
            "file_sep": null,
            "plots": []
        },
        "happy": {
            "pattern": ".*.summary.csv$",
            "file_sep": ",",
            "plots": [
                {
                    "data_type": "SNP",
                    "col_x": "METRIC.Recall",
                    "col_y": "METRIC.Precision",
                    "x_range_low": null,
                    "x_range_high": null,
                    "y_range_low": null,
                    "y_range_high": null,
                    "x_warning_line": [0.999],
                    "x_fail_line": [0.990],
                    "y_warning_line": [0.995],
                    "y_fail_line": null
                },
                {
                    "data_type": "INDEL",
                    "col_x": "METRIC.Recall",
                    "col_y": "METRIC.Precision",
                    "x_range_low": null,
                    "x_range_high": null,
                    "y_range_low": null,
                    "y_range_high": null,
                    "x_warning_line": [0.850],
                    "x_fail_line": [0.820],
                    "y_warning_line": [0.620],
                    "y_fail_line": null
                }
            ]
        }
    }
}
