{
    "project_search": {
        "assay": "CEN",
        "pattern": "^038.*CEN38$",
        "after_date": null,
        "before_date": null,
        "mode": "regexp",
        "number_of_projects": 20
    },
    "file": {
        "flagstat": {
            "pattern": "multiqc_samtools_flagstat.txt",
            "plots": [
                {
                    "col_name": "mapped_passed",
                    "y_range_low": null,
                    "y_range_high": null,
                    "plot_failed": true,
                    "warning_line": null,
                    "fail_line": 2e7,
                    "plot_std": true
                },
                {
                    "col_name": "with mate mapped to a different chr_passed",
                    "y_range_low": null,
                    "y_range_high": null,
                    "plot_failed": true,
                    "warning_line": null,
                    "fail_line": null,
                    "plot_std": true
                }
            ]
        },
        "verifybamid": {
            "pattern": "multiqc_verifybamid.txt",
            "plots": [
                {
                    "col_name": "FREEMIX",
                    "y_range_low": null,
                    "y_range_high": null,
                    "plot_failed": true,
                    "warning_line": null,
                    "fail_line": 0.05,
                    "plot_std": true
                }
            ]
        },
        "picard": {
            "pattern": "multiqc_picard_HsMetrics.txt",
            "plots": [
                {
                    "col_name": "PCT_TARGET_BASES_20X",
                    "y_range_low": null,
                    "y_range_high": null,
                    "plot_failed": true,
                    "warning_line": null,
                    "fail_line": 0.9,
                    "plot_std": true
                },
                {
                    "col_name": "FOLD_80_BASE_PENALTY",
                    "y_range_low": null,
                    "y_range_high": null,
                    "plot_failed": true,
                    "warning_line": 1.3,
                    "fail_line": 1.5,
                    "plot_std": true
                },
                {
                    "col_name": "FOLD_ENRICHMENT",
                    "y_range_low": null,
                    "y_range_high": null,
                    "plot_failed": true,
                    "warning_line": null,
                    "fail_line": null,
                    "plot_std": true
                }
            ]
        },
        "het_hom": {
            "pattern": "multiqc_het-hom_(table|analysis).txt",
            "plots":[
                {
                    "col_name": "mean het ratio",
                    "y_range_low": null,
                    "y_range_high": null,
                    "plot_failed": true,
                    "warning_line": null,
                    "fail_line": 0.47,
                    "plot_std": true
                }
            ]
        },
        "qc_status": {
            "pattern": "^(?i).*_QC_Status\\.xlsx$",
            "plots": []
        }
    }
}
