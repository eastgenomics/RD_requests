# count number of differences between old and new static beds for 2 test cases #
# shows each static bed has one difference between old and new 
$ grep ">" 20251106_difference_between_old_and_new_static_bed_panel.txt | wc -l
4

# make test cases
# test case for no differences in a static bed - shows count of 4 which is expected as 5th case is identical
$ echo -e "2\t40300076\t40301306\tNM_000052.0\n1\t40301800\t40301890\tNM_001000.0" > v1.0.0_SNV_test_1.bed 
$ echo -e "2\t40300076\t40301306\tNM_000052.0\n1\t40301800\t40301890\tNM_001000.0" > v1.0.1_SNV_test_1.bed
$ cp 20251106_difference_between_old_and_new_static_bed_panel.txt 20251106_difference_between_old_and_new_static_bed_panel_test_1.txt
$ diff -s v1.0.0_SNV_test_1.bed v1.0.1_SNV_test_1.bed >> 20251106_difference_between_old_and_new_static_bed_panel_test_1.txt 
$  grep ">" 20251106_difference_between_old_and_new_static_bed_panel_test_1.txt | wc -l
4
$ cat v1.0.0_SNV_test_1.bed
2       40300076        40301306        NM_000052.0
1       40301800        40301890        NM_001000.0
$ cat v1.0.1_SNV_test_1.bed
2       40300076        40301306        NM_000052.0
1       40301800        40301890        NM_001000.0


# test case for more than one difference in a static bed - shows count of 6 which is expected the 5th case has 2 differences
$ cp 20251106_difference_between_old_and_new_static_bed_panel.txt 20251106_difference_between_old_and_new_static_bed_panel_test_2.txt 
$ echo -e "4\t40301176\t40301396\tNM_001092.4\n4\t40301800\t40301994\tNM_001002.4\n6\t40303336\t403053333\tNM_001852.2" > v1.0.0_SNV_test_2.bed
$ echo -e "#assembly=GRCh38,version=v1.0.1\n4\t40301176\t40301396\tNM_001092.4\n4\t40301800\t40301994\tNM_001002.4\n6\t40303336\t403075333\tNM_001852.2" > v1.0.1_SNV_test_2.bed
$ diff -s v1.0.0_SNV_test_2.bed v1.0.1_SNV_test_2.bed >> 20251106_difference_between_old_and_new_static_bed_panel_test_2.txt 
$ grep ">" 20251106_difference_between_old_and_new_static_bed_panel_test_2.txt | wc -l
6


$ cat v1.0.0_SNV_test_2.bed
4       40301176        40301396        NM_001092.4
4       40301800        40301994        NM_001002.4
6       40303336        403053333       NM_001852.2


$ cat v1.0.1_SNV_test_2.bed
#assembly=GRCh38,version=v1.0.1
4       40301176        40301396        NM_001092.4
4       40301800        40301994        NM_001002.4
6       40303336        403075333       NM_001852.2